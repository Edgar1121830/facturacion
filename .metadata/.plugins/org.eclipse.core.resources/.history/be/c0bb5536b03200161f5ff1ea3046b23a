		</query>
	</named-query>

	<named-query name="Estadocaso.EDITAR_EST_CAS">
		<query>	
			update Estadocaso ec set ec.nombre=:nombre, ec.estado=:estado, ec.asignacion=:asignacion where ec.id=:id
		</query>
	</named-query>
	
