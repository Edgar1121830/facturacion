	<named-native-query name="Estadocaso.ID_MAX" result-set-mapping="ec">
		<query>
			select max(e.id)+1 as num
			from ESTADOCASO e
		</query>
	</named-native-query>
	
	<named-native-query name="Pai.MAX_PAIS" result-set-mapping="result">
