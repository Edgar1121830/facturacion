			e.printStackTrace();
			FacesContext.getCurrentInstance().addMessage(null, new FacesMessage(FacesMessage.SEVERITY_ERROR,"",prop.getProperty("errorCregG")));
		}
	}
	
	public void editarTipGas(Tipogasto tg){
